<%= simple_form_for (@walk) do |f| %>
  <div class="form-inputs">
    <%= f.input :starting_point, label: false, placeholder: "From" %>
    <%= f.input :duration, label: false, placeholder: "Timing" %>
    <%= f.input :start_time, label: false, placeholder: "At" %>
    <%= f.input :difficulty, as: :select, collection: Walk::DIFFICULTIES, placeholder: "Difficulty" %>
    <%= f.input :description, label: false, placeholder: "Description" %>
    <%= f.input :frequency, as: :select, collection: Walk::FREQUENCIES, placeholder: "Frequency" %>
    <%= f.input :number_of_participant, label: false, placeholder: "Number of participant"%>
    <%= f.submit class: "btn btn-outline-secondary" %>

  </div>
<% end  %>
